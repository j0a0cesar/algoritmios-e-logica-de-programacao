#include<stdio.h>
int main()
{
  int origem, destino;
  int M[4][4] = {{0,10,8,20},
                {10,0,25,30},
                {8,25,0,12},
                {20,30,12,0}};
  printf("Digite a cidade de origem: ");
  scanf(" %d", &origem);
  
  printf("Digite a ciade de destino: ");
  scanf(" %d", &destino);

  printf("A distancia entre as cidades Ã©: %d", M[origem][destino]);

  return 0;
  }
